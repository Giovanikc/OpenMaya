shader OSLInterface(vector inColor = 1, float inFloat = 0.0, int inInt = 0, vector inVector = 0, output color Cout = 0)
{	
	if( isconnected(inColor) )
	{
		Cout = color(inColor) ;
	}
	if( isconnected(inFloat) )
	{
		Cout = inFloat;
	}
	if( isconnected(inInt) )
	{
		Cout = inInt;
	}
	if( isconnected(inVector) )
	{
		Cout = color(inVector);
	}
}
